        {% for script in scripts %}
            <script type="module" src="/{{ base }}/{{ script.getRelativePathname() }}"></script>
        {% endfor %}
    </head>
    <body>
        <header id="header">
            <nav>
                <a href="/">Home</a>
                <a href="/analytics">Analytics</a>
                <a href="/about">About</a>
            </nav>
        </header>
        <main id="content">

            <div id="calendar"></div>

        </main>
        <footer id="footer"></footer>
    </body>
</html>
